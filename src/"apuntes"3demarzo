#include<stdio.h>
int main (){
int a [5];
//espacios de memoria

printf("%d\n", a);
printf("%d\n", &a);
printf("%d\n", &a[0]);
printf("%d\n", &a[1]);
printf("%d\n", &a[2]);
printf("%d\n", &a[3]);
printf("%d\n", &a[4]);
printf("%d\n", &a[5]);

int numeros[5]={10,20,30,40,50};
for (int i =0; i<5; i++){
    printf("Elemento de %d: %d\n", i, numeros [i]);
}

return 0;
}
